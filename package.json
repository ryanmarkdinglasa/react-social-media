{
  "name": "innosoft-boilerplate",
  "version": "1.0.0",
  "description": "Innosoft software boilerplate",
  "main": "./out/main/index.js",
  "author": "Mark Dinglasa",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@mdxeditor/editor": "^2.9.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/styled-engine": "^5.15.11",
    "@syncfusion/ej2": "^24.2.9",
    "@syncfusion/ej2-base": "^24.2.8",
    "@syncfusion/ej2-react-calendars": "^24.2.9",
    "@syncfusion/ej2-react-charts": "^24.2.9",
    "@syncfusion/ej2-react-dropdowns": "^24.2.9",
    "@syncfusion/ej2-react-grids": "^24.2.9",
    "@syncfusion/ej2-react-inputs": "^24.2.9",
    "@syncfusion/ej2-react-kanban": "^24.2.9",
    "@syncfusion/ej2-react-popups": "^24.2.10",
    "@syncfusion/ej2-react-richtexteditor": "^24.2.9",
    "@syncfusion/ej2-react-schedule": "^24.2.8",
    "jotai": "^2.6.5",
    "npm": "^10.5.0",
    "postcss-import": "^16.0.1",
    "react-router-dom": "^6.22.2",
    "react-tooltip": "^5.26.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
